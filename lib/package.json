{
  "name": "jotai-idb",
  "version": "0.0.5",
  "description": "A typed, reactive IndexedDB state manager powered by Jotai. Persist and sync your application state with minimal boilerplate, full type safety, and Jotai's atom-based architecture.",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "rstest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@rslib/core": "^0.18.2",
    "@rstest/core": "^0.6.6",
    "@types/node": "^24.10.1",
    "fake-indexeddb": "^6.2.5",
    "fast-deep-equal": "3.1.3",
    "react": "^19.2.0"
  },
  "peerDependencies": {
    "jotai": ">=2.0.0",
    "react": ">=18.0.0"
  },
  "peerDependencyMeta": {
    "react": {
      "optional": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rewdy/jotai-idb.git"
  },
  "bugs": {
    "url": "https://github.com/rewdy/jotai-idb/issues"
  },
  "homepage": "https://github.com/rewdy/jotai-idb#readme",
  "keywords": [
    "jotai",
    "indexeddb",
    "state-management",
    "reactive",
    "typescript",
    "react",
    "storage",
    "database"
  ],
  "publishConfig": {
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Andrew Meyer",
  "license": "MIT"
}
